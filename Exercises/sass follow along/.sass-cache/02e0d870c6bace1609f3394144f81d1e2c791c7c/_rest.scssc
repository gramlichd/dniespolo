    q {
        quotes: none;
    }
}

// 2
q:before,
q:after {
    content: '';
    content: none;
}

// Address inconsistent and variable font size in all browsers.
 
small {
    font-size: 80%;
}

// Prevents sub and sup affecting line-height in all browsers
